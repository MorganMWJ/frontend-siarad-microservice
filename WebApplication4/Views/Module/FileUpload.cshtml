@model FileUploadModel

@{ ViewBag.Title = "File Upload";
}

<h1>File Upload</h1>


<form asp-action="Create" enctype="multipart/form-data">

    <div class="form-group row">
        <label asp-for="UploadModule" class="col-sm-2 col-form-label">Upload Module File: </label>
        <div class="col-sm-10">
            <input asp-for="UploadModule" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="UploadStaff" class="col-sm-2 col-form-label">Upload Staff File: </label>
        <div class="col-sm-10">
            <input asp-for="UploadStaff" />
        </div>
        <div class="col-sm-10">
            <select asp-for="CampusCode" asp-items="Model.CampusCodes"></select>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="UploadStudentModule" class="col-sm-2 col-form-label">Upload Student Module File: </label>
        <div class="col-sm-10">
            <input asp-for="UploadStudentModule" />
        </div>
        <div class="col-sm-10">
            <select asp-for="CampusCode" asp-items="Model.CampusCodes"></select>
        </div>
        <div class="col-sm-10">
            <label asp-for="Year" class="col-sm-2 col-form-label">Year: </label>
            <div class="col-sm-10">
                <input asp-for="Year" />
            </div>
        </div>

    </div>
    
   
    

    <input type="submit" />
</form>

